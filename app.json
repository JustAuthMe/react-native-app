{
  "expo": {
    "name": "JustAuth.Me",
    "slug": "just-auth-me",
    "scheme": "jam",
    "privacy": "unlisted",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.1.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "me.justauth.mobile.ios",
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "We use FaceID in order to authenticate the user before loging them into the selected website or service"
      }
    },
    "android": {
      "package": "me.justauth.mobile.android"
    }
  }
}
